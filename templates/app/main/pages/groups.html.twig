{% extends 'app/base.html.twig' %}
{% block body %}
    {% if document.title1 or document.title2 %}
    <div class="text-center pt-4">
        <div class="container">
            <div class="row">
                <div class="mx-auto col-md-12">
                    <h1 class="mb-3">{{ document.title1 }}</h1>
                    <p class="gr_text">{{ document.title2 }}</p>
                </div>
            </div>
        </div>
    </div>
    {% endif %}


    <div class="container mb-3 mt-4">
        <div class="row">
            <div class="col-12">
                <nav aria-label="breadcrumb">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="{{ path('homepage') }}">Home</a></li>
                        <li class="breadcrumb-item active" aria-current="page">{{ document.pageName }}</li>
                    </ol>
                </nav>
            </div>
        </div>
    </div>



    <div class="container">
        <div class="bg_white white_box p-4 more-box">
            <h4 class="mb-3 page-title text-center">{{ document.blockByFieldKey('title1') }}</h4>
            <img data-src="{{ document.blockByFieldKey('leftimage1') }}" class="mb-3 mt-2 img-p-s lazy">
            <div class="desc-wrapper no-more-for-desc">
                <div class="desc">
                    {{ document.blockByFieldKey('text1') | raw }}
                </div>
            </div>
            <div class="clearfix"></div>
        </div>


        <div class="text-center p-4 blue_text_t">
            {{ document.blockByFieldKey('text2') | raw }}
        </div>

        <div class="container">
            <div class="row">

                    <iframe id="JotFormIFrame" onload="window.parent.scrollTo(0,0)" width="100%" allowtransparency="true" src="https://form.jotformpro.com/form/52253331278957" frameborder="0" style="width: 100%; height: 3200px; border: none; opacity: 1; visibility: visible;" scrolling="yes"></iframe>

            </div>
        </div>

    </div>
    <div class="container mt-4">
        <hr>
    </div>
    <div class="col-lg-12">

        <div class="row">
            <div class="col-12 col-lg-12">
                <div class="text-center pt-4">
                    <div class="">
                        <div class="row">
                            {% include 'app/main/snip/review.html.twig' %}
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-12 col-lg-6 offset-lg-3 mt-5">
                <div class="text-center py-4 footer_icons">
                    <div class="container">
                        <div class="row">
                            <div class="col-12">

                                <div class="row">
                                    {% include 'app/main/snip/menu_pagebottom.twig' %}
                                </div>

                            </div>
                        </div>

                    </div>
                </div>
            </div>

        </div>


    </div>
    <div class="mt-5"></div>
{% endblock %}