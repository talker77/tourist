{% extends 'app/base.html.twig' %}
{% block body %}

    {% if document.title1 or document.title2 %}
        <div class="text-center pt-4">
            <div class="container">
                <div class="row">
                    <div class="mx-auto col-md-12">
                        <h1 class="mb-3">{{ document.title1 }}</h1>
                        <p class="gr_text">{{ document.title2 }}</p>
                    </div>
                </div>
            </div>
        </div>
    {% endif %}



    <div class="container mb-3 mt-4">
        <div class="row">
            <div class="col-12">
                <nav aria-label="breadcrumb">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="{{ path('homepage') }}">Home</a></li>
                        <li class="breadcrumb-item active" aria-current="page">{{ document.pageName }}</li>
                    </ol>
                </nav>
            </div>
        </div>
    </div>






    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="">
                    <div class=" text-center">
                        <div class="">


                            <div class="row">

                                <div class="col-6 col-md-4 mb-4">
                                    <div class="bg_white p-3 about_number_box d-flex align-items-center">
                                        <div>
                                            {{ document.blockByFieldKey('box1') | raw }}
                                        </div>
                                    </div>
                                </div>

                                <div class="col-6 col-md-4 mb-4">
                                    <div class="bg_white p-3 about_number_box d-flex align-items-center">
                                        <div>
                                            {{ document.blockByFieldKey('box2') | raw }}
                                        </div>
                                    </div>
                                </div>


                                <div class="col-6 col-md-4 mb-4">
                                    <div class="bg_white p-3 about_number_box d-flex align-items-center">
                                        <div>
                                            {{ document.blockByFieldKey('box3') | raw }}
                                        </div>
                                    </div>
                                </div>


                                <div class="col-6 col-md-4 mb-4">
                                    <div class="bg_white p-3 about_number_box d-flex align-items-center">
                                        <div>
                                            {{ document.blockByFieldKey('box4') | raw }}
                                        </div>
                                    </div>
                                </div>


                                <div class="col-6 col-md-4 mb-4">
                                    <div class="bg_white p-3 about_number_box d-flex align-items-center">
                                        <div>
                                            {{ document.blockByFieldKey('box5') | raw }}
                                        </div>
                                    </div>
                                </div>


                                <div class="col-6 col-md-4 mb-4">
                                    <div class="bg_white p-3 about_number_box d-flex align-items-center">
                                        <div>
                                            {{ document.blockByFieldKey('box6') | raw }}
                                        </div>
                                    </div>
                                </div>
                                <div style="clear: both"></div>
                            </div>


                            <div class="row">
                                <div class="col-12">
                                    <div class="bg_white p-3 mb-4 text-left">
                                        <img data-src="{{ document.blockByFieldKey('right_image') }}" class="mb-3 img-p-s lazy">
                                        {{ document.blockByFieldKey('text1') | raw }}
                                    </div>
                                </div>
                            </div>


                            <div class="row">
                                <div class="col-12">
                                    <div class="">
                                        {{ render(controller(
                                            'App\\Controller\\App\\PartialController::faq', {max: 3}
                                        )) }}
                                        <div class="mt-4">&nbsp;</div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="container-fluid">
        <hr>
    </div>
    <div class="col-lg-12">

        <div class="row">
            <div class="col-12 col-lg-12">
                <div class="text-center pt-4">
                    <div class="">
                        <div class="row">
                            {% include 'app/main/snip/review.html.twig' %}
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-12 col-lg-6 offset-lg-3 mt-5">
                <div class="text-center py-4 footer_icons">
                    <div class="container">
                        <div class="row">
                            <div class="col-12">

                                <div class="row">
                                    {% include 'app/main/snip/menu_pagebottom.twig' %}
                                </div>

                            </div>
                        </div>

                    </div>
                </div>
            </div>

        </div>


    </div>
{% endblock %}