


{% set navigation = 'page_bottom_menu'|navigationTree %}
                        {% if navigation.items |length %}
                            {% for parent,item in navigation.items.0 %}
                                <div class="mx-auto col-4 col-md-4">
                                    <a href="{{ item|alink }}"><h5 class="">{{ item.name }}</h5></a>
                                </div>
                            {% endfor %}
                        {% endif %}
