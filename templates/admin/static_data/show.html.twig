{% extends 'base.html.twig' %}

{% block title %}StaticData{% endblock %}

{% block body %}
    <h1>StaticData</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ static_datum.id }}</td>
            </tr>
            <tr>
                <th>Key</th>
                <td>{{ static_datum.key }}</td>
            </tr>
            <tr>
                <th>Value</th>
                <td>{{ static_datum.value }}</td>
            </tr>
            <tr>
                <th>Category</th>
                <td>{{ static_datum.category }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ static_datum.createdAt ? static_datum.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('static_data_index') }}">back to list</a>

    <a href="{{ path('static_data_edit', {'id': static_datum.id}) }}">edit</a>

    {{ include('static_data/_delete_form.html.twig') }}
{% endblock %}
