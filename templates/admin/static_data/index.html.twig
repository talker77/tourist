{% extends 'admin/base.html.twig' %}
{% block content %}
<div class="content-viewport col-md-12">
    <div class="row">
        <div class="col-md-12">
            <div class="grid">
                <div class="grid-header">StaticData</div>
                <div class="grid-body">

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Key</th>
                <th>Value</th>
                <th>Category</th>
                <th>CreatedAt</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for static_datum in static_datas %}
            <tr>
                <td>{{ static_datum.id }}</td>
                <td>{{ static_datum.key }}</td>
                <td>{{ static_datum.value }}</td>
                <td>{{ static_datum.category }}</td>
                <td>{{ static_datum.createdAt ? static_datum.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('static_data_edit', {'id': static_datum.id}) }}" class="btn btn-success">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('static_data_new') }}">Create new</a>

                </div>
                </div>
                </div>
                </div>
                </div>
                </div>
{% endblock %}
